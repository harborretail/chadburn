{
  "name": "chadburn",
  "version": "0.2.0-rc.0",
  "description": "A nodejs interface to communicate with NetworkManager and ModemManager via DBus",
  "keywords": [
    "dbus",
    "modem",
    "modemmanager",
    "networkmanager"
  ],
  "author": "Nathan Pipe",
  "repository": {
    "type": "git",
    "url": "https://github.com/harborretail/chadburn"
  },
  "homepage": "https://harborretail.github.io/chadburn",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc -p tsconfig.json",
    "test": "mocha -r ts-node/register --require test/src/hooks.ts 'test/src/*.ts'",
    "docs": "npx typedoc ./src --readme none",
    "prepublish": "npm run build && npm run docs"
  },
  "files": [
    "lib/"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.18",
    "@types/uuid": "^8.3.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^5.3.0",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.6",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/dbus": "^1.0.4",
    "dbus": "^1.0.7",
    "rxjs": "^6.6.3",
    "uuid": "^8.3.1"
  },
  "os": [
    "linux"
  ]
}
